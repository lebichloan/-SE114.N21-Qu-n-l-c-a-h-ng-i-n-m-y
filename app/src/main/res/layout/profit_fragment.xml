<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".reports.Sell_fragment">

    <!-- TODO: Update blank fragment layout -->
    <TextView
        android:id="@+id/tvFund"
        android:layout_width="391dp"
        android:layout_height="wrap_content"
        android:text="Báo cáo sổ quỹ"
        android:textSize="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="35dp"
        android:drawableLeft="@mipmap/fund_foreground"
        android:textAlignment="center"
        android:textStyle="bold"
        android:textColor="#1976D2"/>

    <TextView
        android:id="@+id/tvCustomerDebt"
        android:layout_width="391dp"
        android:layout_height="wrap_content"
        android:text="Công nợ khách hàng"
        android:textSize="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="30dp"
        android:drawableLeft="@mipmap/cus_deft_foreground"
        android:textAlignment="center"
        android:textStyle="bold"
        android:textColor="#1976D2"/>

    <TextView
        android:id="@+id/tvSupplierDebt"
        android:layout_width="391dp"
        android:layout_height="wrap_content"
        android:text="Công nợ nguồn cung"
        android:textSize="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="30dp"
        android:drawableLeft="@mipmap/sup_deft_foreground"
        android:textAlignment="center"
        android:textStyle="bold"
        android:textColor="#1976D2"/>

</LinearLayout>